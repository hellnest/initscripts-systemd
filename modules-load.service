[Unit]
Description=Load modules
DefaultDependencies=no
Conflicts=shutdown.target
Before=sysinit.target sysinit.service shutdown.target

[Service]
ExecStart=/lib/systemd/modules-load
ExecStop=/sbin/modprobe -r psmouse
RemainAfterExit=yes
Type=oneshot
